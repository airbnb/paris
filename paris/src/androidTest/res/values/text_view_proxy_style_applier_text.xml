<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:tools="http://schemas.android.com/tools">

    <style name="TextViewStyleApplierTest_drawables">
        <item name="android:drawableBottom">@android:drawable/ic_delete</item>
        <item name="android:drawableLeft">@android:drawable/ic_delete</item>
        <item name="android:drawableRight">@android:drawable/ic_delete</item>
        <item name="android:drawableTop">@android:drawable/ic_delete</item>
    </style>

    <dimen name="text_view_proxy_style_applier_test_drawable_padding">4dp</dimen>
    <style name="TextViewStyleApplierTest_compoundDrawablePadding">
        <item name="android:drawablePadding">@dimen/text_view_proxy_style_applier_test_drawable_padding</item>
    </style>

    <style name="TextViewStyleApplierTest_no_drawables">
        <item name="android:textAllCaps">true</item>
    </style>

    <integer name="text_view_proxy_style_applier_test_ellipsize">3</integer>
    <style name="TextViewStyleApplierTest_ellipsize">
        <item name="android:ellipsize">@integer/text_view_proxy_style_applier_test_ellipsize</item>
    </style>

    <style name="TextViewStyleApplierTest_gravity">
        <item name="android:gravity">center</item>
    </style>

    <item format="float" type="dimen" name="text_view_proxy_style_applier_test_letter_spacing">1.4</item>
    <style name="TextViewStyleApplierTest_letterSpacing">
        <item name="android:letterSpacing" tools:targetApi="lollipop">@dimen/text_view_proxy_style_applier_test_letter_spacing</item>
    </style>

    <integer name="text_view_proxy_style_applier_test_lines">4</integer>
    <style name="TextViewStyleApplierTest_lines">
        <item name="android:lines">@integer/text_view_proxy_style_applier_test_lines</item>
    </style>

    <dimen name="text_view_proxy_style_applier_test_line_spacing_extra">4dp</dimen>
    <style name="TextViewStyleApplierTest_lineSpacingExtra">
        <item name="android:lineSpacingExtra">@dimen/text_view_proxy_style_applier_test_line_spacing_extra</item>
    </style>

    <item format="float" type="dimen" name="text_view_proxy_style_applier_test_line_spacing_multiplier">1.5</item>
    <style name="TextViewStyleApplierTest_lineSpacingMultiplier">
        <item name="android:lineSpacingMultiplier">@dimen/text_view_proxy_style_applier_test_line_spacing_multiplier</item>
    </style>

    <integer name="text_view_proxy_style_applier_test_max_lines">3</integer>
    <style name="TextViewStyleApplierTest_maxLines">
        <item name="android:maxLines">@integer/text_view_proxy_style_applier_test_max_lines</item>
    </style>

    <integer name="text_view_proxy_style_applier_test_min_lines">4</integer>
    <style name="TextViewStyleApplierTest_minLines">
        <item name="android:minLines">@integer/text_view_proxy_style_applier_test_min_lines</item>
    </style>

    <dimen name="text_view_proxy_style_applier_test_min_width">99dp</dimen>
    <style name="TextViewStyleApplierTest_minWidth">
        <item name="android:minWidth">@dimen/text_view_proxy_style_applier_test_min_width</item>
    </style>

    <style name="TextViewStyleApplierTest_singleLine">
        <item name="android:singleLine">true</item>
    </style>

    <bool name="text_view_proxy_style_applier_test_text_all_caps">true</bool>
    <style name="TextViewStyleApplierTest_textAllCaps">
        <item name="android:textAllCaps">@bool/text_view_proxy_style_applier_test_text_all_caps</item>
    </style>

    <dimen name="text_view_proxy_style_applier_test_text_size">24sp</dimen>
    <style name="TextViewStyleApplierTest_textSize">
        <item name="android:textSize">@dimen/text_view_proxy_style_applier_test_text_size</item>
    </style>

</resources>
